{
    "name": "mdhesari/laravel-country-state-cities",
    "description": "Laravel country state cities",
    "keywords": [
        "mdhesari",
        "laravel-country-state-cities"
    ],
    "homepage": "https://github.com/mdhesari/laravel-country-state-cities",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamad Hesari",
            "email": "mdhesari99@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mdhesari\\LaravelCountryStateCities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdhesari\\LaravelCountryStateCities\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mdhesari\\LaravelCountryStateCities\\LaravelCountryStateCitiesServiceProvider"
            ],
            "aliases": {
                "LaravelCountryStateCities": "Mdhesari\\LaravelCountryStateCities\\LaravelCountryStateCitiesFacade"
            }
        }
    }
}
